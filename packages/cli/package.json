{
  "name": "@contember/cli",
  "version": "1.4.0-rc.3",
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "contember": "./dist/src/run.js"
  },
  "scripts": {
    "build": "yarn build:js:dev && yarn build:js:prod && yarn build:css",
    "build:css": "tailwindcss -i ./src/utils/project/resources/style.css -o ./dist/src/utils/project/resources/out.css",
    "test": "echo 'No tests'",
    "build:js:dev": "NODE_ENV=development vite build --mode development",
    "build:js:prod": "vite build --mode production"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.0",
    "@types/prompts": "^2.4.8",
    "@types/semver": "^7.5.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  },
  "dependencies": {
    "@contember/cli-common": "workspace:*",
    "@contember/config-loader": "workspace:*",
    "@contember/database": "workspace:*",
    "@contember/dic": "workspace:*",
    "@contember/engine-content-api": "workspace:*",
    "@contember/migrations-client": "workspace:*",
    "@contember/schema": "workspace:*",
    "@contember/schema-migrations": "workspace:*",
    "@contember/schema-utils": "workspace:*",
    "@graphql-tools/schema": "^10.0.0",
    "chalk": "^4.1.2",
    "chalk-table": "^1.0.2",
    "esbuild": "^0.18.20",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "js-yaml": "^4.1.0",
    "pg": "^8.11.3",
    "preact": "^10.19.2",
    "preact-render-to-string": "^6.3.1",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "yawn-yaml": "^2.2.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "development": "./dist/development/index.js",
        "production": "./dist/production/index.js",
        "typescript": "./src/index.ts",
        "default": "./dist/production/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "development": "./dist/development/index.cjs",
        "production": "./dist/production/index.cjs",
        "default": "./dist/production/index.cjs"
      }
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "typings": "./dist/types/index.d.ts",
  "main": "./dist/production/index.js"
}
