{
  "name": "@contember/engine-system-api",
  "version": "1.4.0-rc.3",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/production/index.js",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "development": "./dist/development/index.js",
        "production": "./dist/production/index.js",
        "typescript": "./src/index.ts",
        "default": "./dist/production/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "development": "./dist/development/index.cjs",
        "production": "./dist/production/index.cjs",
        "default": "./dist/production/index.cjs"
      }
    }
  },
  "scripts": {
    "generate": "gql-gen --config graphql.codegen.yml",
    "test": "vitest --dir ./tests/cases",
    "build": "yarn build:js:dev && yarn build:js:prod",
    "build:js:dev": "NODE_ENV=development vite build --mode development",
    "build:js:prod": "vite build --mode production"
  },
  "dependencies": {
    "@contember/authorization": "workspace:*",
    "@contember/database": "workspace:*",
    "@contember/database-migrations": "workspace:*",
    "@contember/dic": "workspace:*",
    "@contember/graphql-utils": "workspace:*",
    "@contember/logger": "workspace:*",
    "@contember/queryable": "workspace:*",
    "@contember/schema": "workspace:*",
    "@contember/schema-migrations": "workspace:*",
    "@contember/schema-utils": "workspace:*",
    "@graphql-tools/utils": "^10.0.8",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@contember/engine-api-tester": "workspace:*",
    "@contember/schema-definition": "workspace:*",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/node": "^20.9.0",
    "graphql": "^16.8.1",
    "pg": "^8.11.3"
  },
  "peerDependencies": {
    "graphql": ">= 14.6.0",
    "pg": "^8.9.0"
  },
  "files": [
    "dist/",
    "src/"
  ]
}
